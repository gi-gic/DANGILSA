            app:destination="@id/mainGeneralFragment2" />
    </fragment>
    <fragment
        android:id="@+id/nav_my_general"
        android:name="com.example.dangillsa_kotlin.MyFragment"
        android:label="fragment_my"
        tools:layout="@layout/fragment_my" >
        <action
            android:id="@+id/action_myFragment_to_mainGeneralFragment2"
            app:destination="@id/mainGeneralFragment2" />
    </fragment>
</navigation>