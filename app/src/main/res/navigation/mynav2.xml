<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mynav2"
    app:startDestination="@id/mainGeneralFragment2">

    <fragment
        android:id="@+id/mainGeneralFragment2"
        android:name="com.example.dangillsa_kotlin.MainGeneralFragment"
        android:label="fragment_main_general"
        tools:layout="@layout/fragment_main_general" >
        <action
            android:id="@+id/action_mainGeneralFragment2_to_helmetFragment2"
            app:destination="@id/helmetFragment2" />
        <action
            android:id="@+id/action_mainGeneralFragment2_to_fallFragment2"
            app:destination="@id/fallFragment2" />
